{
  "compilerOptions": {
    "jsx": "react",
    "module": "es6",
    "target": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowUnreachableCode": false,
    "strict": true,
    "lib": ["es5", "dom", "es6"]
  },
  "include": ["src/**/*.ts", "src/**/*tsx"],
  "exclude": ["node_modules"]
}
